# project name
name="name"
debug=true


[log]
# Available settings: single/daily
type="single"

# Available settings: debug/info/notice/warning/error/critical
level="debug"

# Available settings: file/stdout/stderr
#driver="stderr"
driver = "stdout"

# utc/local
datetime_format = "local"

[cache]
enable=true

[redis]
host="127.0.0.1"
port=6379
db=0


[qiniu]
access_key="access_key"
secret_key="secret_key"
bucket="bucket"
domain="http://bkt.clouddn.com/"

[mail]
account="XXX@example.com"
password="password"

[db.etcd]
host="127.0.0.1"
port=2379

[db.mysql]
driver="mysql"
host="localhost"
port=3306
database="dc"
user="root"
password="123456"
log_queries=true
charset= "utf8"

[db.mongo]
